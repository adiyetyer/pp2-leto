x = int(1)   # x will be 1
y = int(2.8) # y will be 2
z = int("3") # z will be 3
print (x,y,z) 
a = float(1)     # x will be 1.0
b = float(2.8)   # y will be 2.8
c = float("3")   # z will be 3.0
d = float("4.2") # w will be 4.2
print(a,b,c,d)
q = str("s1") # x will be 's1'
w = str(2)    # y will be '2'
e = str(3.0)  # z will be '3.0'
print (q,w,e)