#exercise 1
def func(num):
    res = num / 28.4
    return res

input_value = float(input("Add your name: "))
print("Your result: " + str(func(input_value)))
#exercise 2
def function(num):
    num_C = (5 / 9) * (num - 32)
    return num_C

input_value = float(input("Add your name: "))
print("Your result: " + str(function(input_value)))
#exercise 3
def solve(numheads, numlegs):
    for num_chickens in range(numheads + 1):
        num_rabbits = numheads - num_chickens
        total_legs = 2 * num_chickens + 4 * num_rabbits

        if total_legs == numlegs:
            return num_chickens, num_rabbits
    return None
print(solve(35, 94))