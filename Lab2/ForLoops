#exercise 1
fruits = ["apple", "banana", "cherry"]
for x in fruits:
  print(x)
#exercise 2
Fruits = ["apple", "banana", "cherry"]
for y in Fruits:
  if y == "banana":
    continue
  print(y)
#exercise 3
for z in range(6):
  print(z)
#exercise 4
Fruits = ["apple", "banana", "cherry"]
for a in fruits:
  if a == "banana":    
   break
  print(a)